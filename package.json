{
  "name": "@jsxiaosi/eslint-config",
  "version": "0.0.6",
  "description": "eslint 配置",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "bump package.json packages/*/package.json",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsxiaosi/eslint-config.git#readme"
  },
  "keywords": [
    "eslint"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsxiaosi/eslint-config/issues"
  },
  "homepage": "https://github.com/jsxiaosi/eslint-config/tree/readme#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.23.0",
    "husky": "^8.0.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@jsxiaosi/eslint-config-base": "workspace:*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  }
}
